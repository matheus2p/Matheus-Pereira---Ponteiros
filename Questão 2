#include <stdio.h>
#include <stdlib.h>

int main(){
int i=3,j=5;
int *p, *q;
p = &i;
q = &j;

printf("%d", p == &i); // Ser치 impresso o resultado de 1.
printf("%d",*p - *q); //  Ser치 impresso o resultado de -2.
printf("%d", **&p); //  Ser치 impresso o resultado de 3.
printf("%d", 3 - *p/(*q) + 7); // Ser치 impresso o resultado de 10.

return 0;

}
